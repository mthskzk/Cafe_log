<%= form_with url: admin_searches_search_sns_path, method: :get, local: true do |f| %>
  <h5>ユーザー名検索</h5>
  <div>
    <%= f.text_field :key_word, size: "30",placeholder: "ユーザー名" %>
    <%= f.hidden_field :model, :value => "customer" %>
  </div>
  <div>
    <%= f.submit '検索', class: "btn btn-outline-dark btn-sm" %>
  </div>
<% end %>

